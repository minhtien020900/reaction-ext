{
  "{{chrome}}.manifest_version": 3,
  "{{firefox}}.manifest_version": 2,
  "icons": {
    "16": "icon/sj.png",
    "32": "icon/sj.png",
    "48": "icon/sj.png",
    "96": "icon/sj.png",
    "128": "icon/sj.png"
  },
  "{{chrome}}.action": {
    "default_popup": "src/popup.html"
  },
  "{{firefox}}.browser_action": {
    "default_popup": "src/popup.html"
  },

  "background": {
    "{{chrome}}.service_worker": "src/background/index.js",
    "{{firefox}}.scripts": [
      "src/background/index.js"
    ]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["src/content/index.js","src/plugin/jquery-3.7.1.min.js","src/plugin/bootstrap.bundle.js"]
  }]
}
